{
    "name": "furnishr-consumer-fe2-spa",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "cp src/env/local.ts src/env/env.ts & react-scripts start",
        "build:dev": "cp src/env/dev.ts src/env/env.ts & react-scripts build",
        "deploy:dev": "rm -rf ./build && yarn build:dev && yarn gulp",
        "build:staging": "cp src/env/staging.ts src/env/env.ts & react-scripts build",
        "build:prod": "cp src/env/prod.ts src/env/env.ts & react-scripts build",
        "build:vista": "cp src/env/vista.ts src/env/env.ts & react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "analyze": "source-map-explorer build/static/js/*.js",
        "lint": "lint-staged",
        "lint:ts": "eslint src/**/*.ts",
        "lint:tsx": "eslint src/**/*.tsx",
        "lint:css": "stylelint src/**/*.css"
    },
    "lint-staged": {
        "*.{ts}": "yarn lint:ts",
        "*.{tsx}": "yarn lint:tsx",
        "*.{css}": "yarn lint:css"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "lint-staged"
        }
    },
    "dependencies": {
        "@sentry/browser": "5.4.0",
        "axios": "0.19.0",
        "classnames": "2.2.6",
        "local-storage": "2.0.0",
        "lodash": "4.17.11",
        "normalize.css": "8.0.1",
        "object-to-formdata": "1.6.4",
        "rc-select": "9.1.4",
        "react": "16.8.6",
        "react-dom": "16.8.6",
        "react-dropzone": "10.1.5",
        "react-helmet-async": "1.0.2",
        "react-redux": "7.1.0",
        "react-redux-toastr": "7.5.1",
        "react-scripts": "3.0.1",
        "redux": "4.0.1",
        "redux-form": "8.2.4",
        "redux-saga": "1.0.3",
        "reselect": "4.0.0",
        "typescript": "3.5.2",
        "@types/classnames": "2.2.8",
        "@types/jest": "24.0.15",
        "@types/lodash": "4.14.134",
        "@types/next-redux-saga": "3.0.1",
        "@types/node": "12.0.8",
        "@types/rc-select": "5.9.34",
        "@types/react": "16.8.20",
        "@types/react-dom": "16.8.4",
        "@types/react-dropzone": "4.2.2",
        "@types/react-helmet": "5.0.8",
        "@types/react-redux": "7.1.0",
        "@types/react-redux-toastr": "7.4.1",
        "@types/redux-form": "8.1.4",
        "redux-devtools-extension": "2.13.8"
    },
    "devDependencies": {
        "del": "4.1.1",
        "gulp": "4.0.2",
        "husky": "2.4.1",
        "lint-staged": "8.2.1",
        "source-map-explorer": "2.0.0",
        "stylelint": "10.1.0",
        "stylelint-config-recommended": "2.2.0"
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
